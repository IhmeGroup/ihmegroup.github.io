	pages = {4208--4230},
	file = {Wu et al. - 2015 - A Pareto-efficient combustion framework with submo.pdf:/Users/jjhansen/Zotero/storage/HQ8KG95P/Wu et al. - 2015 - A Pareto-efficient combustion framework with submo.pdf:application/pdf},
}

@article{wu_effects_2014,
	title = {Effects of flow-field and mixture inhomogeneities on the ignition dynamics in continuous flow reactors},
