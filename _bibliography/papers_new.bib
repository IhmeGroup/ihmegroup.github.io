	pages = {753--761},
	file = {Franzelli et al. - 2017 - Numerical investigation of soot-flame-vortex inter.pdf:/Users/jjhansen/Zotero/storage/DPMW8NVW/Franzelli et al. - 2017 - Numerical investigation of soot-flame-vortex inter.pdf:application/pdf},
}

@inproceedings{froute_evaluation_2021,
