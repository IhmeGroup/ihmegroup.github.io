	note = {arXiv:1712.00966 [physics]},
	keywords = {Physics - Fluid Dynamics},
	file = {Wu et al. - 2017 - Application of Pareto-efficient combustion modelin.pdf:/Users/jjhansen/Zotero/storage/3PBCEZZE/Wu et al. - 2017 - Application of Pareto-efficient combustion modelin.pdf:application/pdf},
}

@misc{wu_efficient_2017,
	title = {Efficient time stepping for reactive turbulent simulations with stiff chemistry},
	url = {http://arxiv.org/abs/1712.00953},
	abstract = {A combination of a steady-state preserving operator splitting method and a semi-implicit integration scheme is proposed for eﬃcient time stepping in simulations of unsteady reacting ﬂows, such as turbulent ﬂames, using detailed chemical kinetic mechanisms. The operator splitting is based on the Simpler balanced splitting method, which is constructed with improved stability properties and reduced computational cost. The method is shown to be capable of stable and accurate prediction of ignition and extinction for reaction-diﬀusion systems near critical conditions. The ROK4E scheme is designed for semi-implicit integration of spatially independent chemically reacting systems. Being a Rosenbrock-Krylov method, ROK4E utilizes the low-rank approximation of the Jacobian to reduce the cost for integrating the system of ODEs that have relative few stiﬀ components. The eﬃciency of the scheme is further improved via the careful choice of coeﬃcients to require three right-hand-side evaluations over four stages. Combing these two methods, eﬃcient calculation is achieved for large-scale parallel simulations of turbulent ﬂames.},
	language = {en},
	urldate = {2024-10-28},
	publisher = {arXiv},
	author = {Wu, Hao and Ma, Peter C. and Ihme, Matthias},
	month = dec,
