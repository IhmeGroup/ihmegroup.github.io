	file = {Guo et al. - 2024 - Analysis of ducted fuel injection at high-pressure.pdf:/Users/jjhansen/Zotero/storage/D4DKZD8N/Guo et al. - 2024 - Analysis of ducted fuel injection at high-pressure.pdf:application/pdf},
}

